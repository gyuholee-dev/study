<body>
  <h1>공지사항</h1>
  <table style="width:100%">
    <tr>
      <th>글번호</th>
      <th>글제목</th>
      <th>작성자</th>
      <th>작성일</th>
      <th>조회수</th>
    </tr>
    <% for(let rs of data) { %>
      <tr>
        <td><%= rs.no %></td>
        <td><%= rs.title %></td>
        <td><%= rs.writer %></td>
        <td><%= rs.wrteday %></td>
        <td><%= rs.hit %></td>
      </tr>
    <% } %>
    <tr>
      <td colspan="5" align="center">
        <a href="/insert">공지사항 추가</a>
      </td>
    </tr>
  </table>
</body>